{
  "name": "agents",
  "version": "0.0.1",
  "readme": "https://github.com/langchain-ai/agentic-saas-boilerplate/blob/main/README.md",
  "packageManager": "pnpm@10.5.1",
  "description": "A LangGraph.js ReAct agent with Langgraph Middleware auth using Supabase JWT tokens",
  "main": "src/graph.ts",
  "author": "Langchain",
  "license": "MIT",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/langchain-ai/agentic-saas-boilerplate",
    "directory": "apps/agents"
  },
  "scripts": {
    "build": "tsc",
    "dev": "npx @langchain/langgraph-cli dev --no-browser",
    "clean": "rm -rf dist",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=\\.test\\.ts$ --testPathIgnorePatterns=\\.int\\.test\\.ts$",
    "test:int": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=\\.int\\.test\\.ts$",
    "lint": "eslint src",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@langchain/anthropic": "^1.3.20",
    "@langchain/community": "^1.1.19",
    "@langchain/core": "^1.1.28",
    "@langchain/langgraph": "^1.1.5",
    "@langchain/langgraph-sdk": "^1.6.4",
    "@supabase/supabase-js": "^2.97.0",
    "dotenv": "^17.3.1",
    "langchain": "^1.2.27"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.4",
    "@eslint/js": "^10.0.1",
    "@tsconfig/recommended": "^1.0.13",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.3.0",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  }
}
